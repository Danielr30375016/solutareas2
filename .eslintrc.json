{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:astro/recommended",
      "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "indent": ["warn", "tab"],
      "no-multiple-empty-lines": "off",
      "no-tabs": "off",
      "quotes": ["warn", "single"],
      "jsx-quotes": ["warn", "prefer-single"],
      "eol-last": "off",
      "no-undef": "off"
    }
}
